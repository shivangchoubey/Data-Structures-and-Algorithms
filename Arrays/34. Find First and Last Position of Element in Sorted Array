class Solution {
    public int[] searchRange(int[] nums, int target) {
        int[] result=new int[2];
        int n=nums.length;
        int l=0;
        int r=n-1;
        int fo=-1;
        while(l<=r){
            int mid=l+(r-l)/2;
            if(nums[mid]==target){
              if(mid>0 && nums[mid]==nums[mid-1]) r=mid-1;
              else{
                fo=mid;
                break;
              }
            }
            else if(nums[mid]<target){
                l=mid+1;
            }
            else{
                r=mid-1;      
            }
        }
        l=0;
        r=n-1;
        int lo=-1;
        while(l<=r){
            int mid=l+(r-l)/2;
            if(nums[mid]==target){
                if(mid+1<n && nums[mid]==nums[mid+1]) l=mid+1;
                else{
                    lo=mid;
                    break;
                }
            }
            else if(nums[mid]<target){
                l=mid+1;
            }
            else{
                r=mid-1;
            }
        }
        result[0]=fo;
        result[1]=lo;
        return result;

    }
}
