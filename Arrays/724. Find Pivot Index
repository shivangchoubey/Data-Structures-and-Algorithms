class Solution {
    public int pivotIndex(int[] nums) {
        int sum=0;
        for(int i=0;i<nums.length;i++){
            sum+=nums[i];
        }
        int p=0;
        int lsum=0;
        for(int i=0;i<nums.length;i++){
            int rsum=sum-lsum-nums[i];
            if(rsum==lsum){
                return p;
            }
            lsum+=nums[p];
            p++;
        }
        return -1;
    }
}
