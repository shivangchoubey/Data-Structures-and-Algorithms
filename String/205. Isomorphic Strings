class Solution {
    public boolean isIsomorphic(String s, String t) {
       Map<Character, Character> m=new HashMap<>();
       if(s.length()!=t.length()) return false;
       for(int i=0;i<s.length();i++){
            char k=s.charAt(i);
            char v=t.charAt(i);
            if(!m.containsKey(k)){
                if(m.containsValue(v)){
                    return false;
                }
                m.put(k,v);
            }
            else{
                if(m.get(k)!=v){
                    return false;
                }
            }
        }
         return true;
    
    }
}
