class Solution {
    public char findTheDifference(String s, String t) {
        char s1[]=s.toCharArray();
        char t1[]=t.toCharArray();
        Arrays.sort(s1);
        Arrays.sort(t1);
        int n=s.length();
        int m=t.length();
        int j=0;
        int i=0;
        while(i<m && j<n){
            if(t1[i]==s1[j]){
                j++;
                i++;
            }
            else{
                return t1[i];
            }
        }
        return t1[m-1];
    }
}
